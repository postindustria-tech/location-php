{
    "name": "51degrees/fiftyone.geolocation",
    "description": "Geo-Location engines for 51Degrees Pipeline API - Get postal address details from the device connecting to your website or service",   
    "authors" : [
        {
            "name": "Filip Hn√≠zdo",
            "email": "filip@octophin.com",
            "homepage": "https://octophindigital.com/"
        },
        {
            "name": "Steve Ballantine",
            "email": "stephen@51degrees.com"
        },
        {
            "name": "Ben Shilito",
            "email": "ben@51degrees.com"
        },
        {
            "name": "Joseph Dix",
            "email": "joseph@51degrees.com"
        }
    ],
    "license": "EUPL-1.2", 
    "type": "library",
    "autoload": {
        "psr-4": {
            "fiftyone\\pipeline\\geolocation\\": "src/"
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "pipeline-php-core",
            "options": {
                "versions": {
                    "51degrees/fiftyone.pipeline.core": "4"
                }
            }
        },
        {
            "type": "path",
            "url": "pipeline-php-engines",
            "options": {
                "versions": {
                    "51degrees/fiftyone.pipeline.engines": "4"
                }
            }
        },
        {
            "type": "path",
            "url": "pipeline-php-cloudrequestengine",
            "options": {
                "versions": {
                    "51degrees/fiftyone.pipeline.cloudrequestengine": "4"
                }
            }
        }
    ],
    "require": {
        "51degrees/fiftyone.pipeline.core": "*",
        "51degrees/fiftyone.pipeline.engines": "*",
        "51degrees/fiftyone.pipeline.cloudrequestengine": "*"
    },
    "require-dev": {
        "51degrees/fiftyone.devicedetection": "4.*"
    },
    "minimum-stability": "dev",
    "config": {
        "allow-plugins": {
            "nodejs-php-fallback/nodejs-php-fallback": false
        }
    }
}
